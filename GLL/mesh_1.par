#
# nek parameter file
#
[GENERAL]
stopAt = numSteps
numSteps  = 0

variableDT = yes
targetCFL = 0.5
timeStepper = bdf2

writeControl = timeStep
writeInterval = 5

[PROBLEMTYPE]
equation = incompNS

[PRESSURE]
residualTol = 1e-5
residualProj = yes

[VELOCITY]
residualTol = 1e-8
density = 1
viscosity = 1

[TEMPERATURE]
solver = none
advection = no
